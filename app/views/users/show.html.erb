<h1><%= @user.username %></h1>
<ul>
  <% @user.events.each do |event| %>
    <li><%= event.title %></li>
    <li><%= event.description %></li>
    <li><%= event.address %></li>
    <li><%= event.date %></li>
  <% end %>
</ul>

<ol>
  <% @user.invitations.each do |i| %>
  <li><%= i.attended_event.title %></li>
  <% end %>
</ol>