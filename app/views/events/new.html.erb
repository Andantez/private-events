<%= form_with( model: @event, local: true) do |f| %>
<% if @event.errors.any? %>
  <% @event.errors.full_messages do |msg| %>
  <%= msg %>
  <%end  %>
<% end %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :date %>
  <%= f.date_field :date, value: Date.today%>
  <%= f.submit "Create" %>
<% end %>